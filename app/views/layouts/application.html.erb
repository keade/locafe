<!DOCTYPE html>
<html>
<head>
  <title>Coffee.local.jam <%= yield :title %></title>
  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body>
	<div class="container">
		<div class="header">
			<div class="row">
				<%= render 'layouts/header' %>
			</div>
		</div>
		<div class="content">
			<div class="row">
			<%= yield %>
			</div>
		</div>
	</div>
</body>
</html>
